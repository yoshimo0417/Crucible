/////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////
//            Belchaad the Juggernaut
/////////////////////////////////////////////////////////////////////
//        Version 1
// Belchaad the Juggernaut, 25th level Quasi-Deity
//
// The player will have to kill the three servants to make Belchaad
// vulnerable and able to kill. Belchaad is also a Deity and will be
// able to instant cast spells. Difficulty level will determine the 
// delay between spell casts.
/////////////////////////////////////////////////////////////////////

/////////////////////////////////////////////////////////////////////
// Initiate Defeat Convo
/////////////////////////////////////////////////////////////////////
IF
  Global("MO_DeadSet","LOCALS",1)
  HPLT(Myself,2)
THEN 
  RESPONSE #100
    SetGlobal("MO_DeadSet","LOCALS",2)
    Dialogue([PC])
END

/////////////////////////////////////////////////////////////////////
// Make Vulnerable
/////////////////////////////////////////////////////////////////////
IF
  Dead("mobha24")
  Dead("mobha25")
  Dead("mobha26")
  Global("MO_DeadSet","LOCALS",0)
  DifficultyGT(HARD)
THEN
  RESPONSE #100
    SetGlobal("MO_DeadSet","LOCALS",1)
    //DestroyItem("minhp1")
    DestroyItem("mobha23c")
END

IF
  Dead("mobha24")
  Dead("mobha25")
  //Dead("mobha26")
  Global("MO_DeadSet","LOCALS",0)
  OR(2)
    Difficulty(HARD)
    Difficulty(NORMAL)
THEN
  RESPONSE #100
    SetGlobal("MO_DeadSet","LOCALS",1)
    //DestroyItem("minhp1")
    DestroyItem("mobha23d")
END

IF
  Dead("mobha24")
  Dead("mobha25")
  //Dead("mobha26")
  Global("MO_DeadSet","LOCALS",0)
  DifficultyLT(NORMAL)
THEN
  RESPONSE #100
    SetGlobal("MO_DeadSet","LOCALS",1)
    //DestroyItem("minhp1")
    DestroyItem("mobha23c")
    DestroyItem("mobha23d")
END

/////////////////////////////////////////////////////////////////////
// Initial Set-Up
/////////////////////////////////////////////////////////////////////
IF
  See([GOODCUTOFF])
  Global("Prep","LOCALS",0)
  Allegiance(Myself,ENEMY)
  DifficultyGT(HARD)
THEN
  RESPONSE #100
    SetGlobal("Prep","LOCALS",1)
    SetGlobalTimer("MO_VengeanceStormTimer","LOCALS",12)
    ApplySpell(Myself,CLERIC_RIGHTEOUS_MAGIC)  // SPPR513.SPL (Righteous Magic)
    // IWD Prebuffs
    ApplySpell(Myself,CLERIC_GLOBE_OF_BLADES)  // SPPR7xx.SPL (Globe of Blades)
    ApplySpell(Myself,CLERIC_SHIELD_OF_THE_ARCHONS)  // SPPR701.SPL (Shield of the Archons)
    XEquipItem("mobha23a",Myself,SLOT_WEAPON0,EQUIP)
END

IF
  See([GOODCUTOFF])
  Global("Prep","LOCALS",0)
  Allegiance(Myself,ENEMY)
  DifficultyLT(HARDEST)
THEN
  RESPONSE #100
    SetGlobal("Prep","LOCALS",1)
    ApplySpell(Myself,CLERIC_DRAW_UPON_HOLY_MIGHT)  // SPPR214.SPL (Draw Upon Holy Might)
    //ApplySpell(Myself,CLERIC_BLADE_BARRIER)  // SPPR603.SPL (Blade Barrier)
    ApplySpell(Myself,CLERIC_SHIELD_OF_THE_ARCHONS)  // SPPR701.SPL (Shield of the Archons)
    XEquipItem("mobha23f",Myself,SLOT_WEAPON1,EQUIP)
END

/////////////////////////////////////////////////////////////////////
// Setup Spellcast Timer
/////////////////////////////////////////////////////////////////////
IF
  Global("MO_SpellCast","LOCALS",1)
  DifficultyGT(HARD)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",4)
    SetGlobal("MO_SpellCast","LOCALS",0)
    Continue()
END

IF
  Global("MO_SpellCast","LOCALS",1)
  Difficulty(HARD)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",6)
    SetGlobal("MO_SpellCast","LOCALS",0)
    Continue()
END

IF
  Global("MO_SpellCast","LOCALS",1)
  Difficulty(NORMAL)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",12)
    SetGlobal("MO_SpellCast","LOCALS",0)
    Continue()
END

IF
  Global("MO_SpellCast","LOCALS",1)
  DifficultyLT(NORMAL)
THEN
  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",18)
    SetGlobal("MO_SpellCast","LOCALS",0)
    Continue()
END

// IWD Casting

/////////////////////////////////////////////////////////////////////
// Spell: Shield of the Archons
/////////////////////////////////////////////////////////////////////
IF
  OR(2)
    !GlobalTimerNotExpired("MO_SpellCastTimer","LOCALS")
    CheckStatGT(Myself,0,AURACLEANSING)
  See([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  !CheckStat(Myself,1,CLERIC_SHIELD_OF_THE_ARCHONS)
  !CheckStatGT(Myself,0,CLERIC_INSECT_PLAGUE)
  !StateCheck(Myself,STATE_POISONED)
  !StateCheck(Myself,STATE_SILENCED)
THEN
  RESPONSE #80
    SetGlobal("MO_SpellCast","LOCALS",1)
    SetInterrupt(FALSE)
    ReallyForceSpell(Myself,CLERIC_SHIELD_OF_THE_ARCHONS)  // SPPR701.SPL (Shield of the Archons)
    SetInterrupt(TRUE)
  RESPONSE #20
    Continue()
END

